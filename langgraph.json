{
    "version": "0.1.0",
    "agents": {
        "default_model": "gpt-4",
        "temperature": 0.7,
        "max_tokens": 1000
    },
    "orchestration": {
        "max_steps": 10,
        "timeout_seconds": 300
    },
    "tools": {
        "enabled": true,
        "allow_code_execution": false
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
}