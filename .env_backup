# Application Settings
APP_NAME=AgentOrchestrator
DEBUG=false
HOST=0.0.0.0
PORT=8000

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/agentorchestrator

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Monitoring
ENABLE_PROMETHEUS=true
PROMETHEUS_PORT=9090

# Google AI Configuration
GOOGLE_API_KEY=<your-google-api-key>

# Logging
LOG_LEVEL=INFO

# Authentication
AUTH_ENABLED=true
AUTH_API_KEY_HEADER=X-API-Key
AUTH_DEFAULT_KEY=ao-dev-key-123  # Development API key

# Rate Limiting
RATE_LIMIT_ENABLED=false
RATE_LIMIT_RPM=60
RATE_LIMIT_BURST=100

# Caching
CACHE_ENABLED=false
CACHE_TTL=300  # 5 minutes

# Metrics
METRICS_ENABLED=true
METRICS_PREFIX=ao 